{
  "name": "ajr-relay-layouts",
  "version": "0.0.0",
  "description": "NodeCG layout for all japan relay race",
  "homepage": "",
  "author": {
    "name": "Cma",
    "email": "chicchaima@gmail.com"
  },
  "files": [
    "dashboard",
    "graphics",
    "extension"
  ],
  "keywords": [
    "node.js",
    "nodecg",
    "react",
    "nodecg-bundle"
  ],
  "scripts": {
    "autofix": "run-s autofix:*",
    "autofix:browser": "npm run lint:browser -- --fix",
    "autofix:nodecg": "npm run lint:nodecg -- --fix",
    "build": "webpack --mode=production",
    "lint": "run-s lint:*",
    "lint:browser": "eslint --ext .ts,.tsx src/browser",
    "lint:nodecg": "eslint --ext .ts src/nodecg",
    "dev": "webpack --watch --mode=development",
    "schema-types": "nodecg schema-types -o src/nodecg/generated schemas",
    "schema-types:lib": "nodecg schema-types -o src/nodecg/generated/lib schemas/lib",
    "test": "jest"
  },
  "nodecg": {
    "compatibleRange": ">=1.9.0",
    "dashboardPanels": [
      {
        "name": "cp-result",
        "title": "01:情報一覧",
        "file": "cp-result.html",
        "fullbleed": true
      },
      {
        "name": "timekeep",
        "title": "02:タイムキーパー",
        "file": "timekeep.html",
        "fullbleed": true
      },
      {
        "name": "timeview",
        "title": "03:チェックポイント一覧",
        "file": "timeview.html",
        "fullbleed": true
      },
      {
        "workspace": "ZZ:注意操作系",
        "name": "admin",
        "title": "管理用",
        "file": "admin.html",
        "width": 4
      }
    ],
    "graphics": [
      {
        "file": "main.html",
        "width": "1920",
        "height": "972",
        "singleInstance": false
      },
      {
        "file": "feature.html",
        "width": "1920",
        "height": "972",
        "singleInstance": false
      },
      {
        "file": "omnibar.html",
        "width": "1920",
        "height": "108",
        "singleInstance": false
      }
    ],
    "assetCategories": []
  },
  "repository": "https://github.com/cma2819/ome-speedrun-layouts.git",
  "license": "MIT",
  "dependencies": {
    "@discordjs/voice": "^0.16.0",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@fortawesome/fontawesome-free": "^5.15.3",
    "@fortawesome/fontawesome-svg-core": "^6.4.2",
    "@fortawesome/free-solid-svg-icons": "^6.4.2",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@mui/icons-material": "^5.0.3",
    "@mui/material": "^5.14.6",
    "chart.js": "^4.4.0",
    "clone": "^2.1.2",
    "discord.js": "^14.13.0",
    "globby": "^11.0.3",
    "moment": "^2.29.4",
    "react": "^17.0.2",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^17.0.2",
    "react-transition-group": "^4.4.5",
    "styled-components": "^5.3.0"
  },
  "devDependencies": {
    "@types/clone": "^2.1.1",
    "@types/globby": "^9.1.0",
    "@types/html-webpack-plugin": "^3.2.5",
    "@types/jest": "^27.0.2",
    "@types/node": "^15.6.2",
    "@types/react": "^17.0.9",
    "@types/react-dom": "^17.0.6",
    "@types/react-transition-group": "^4.4.3",
    "@types/styled-components": "^5.1.10",
    "@types/webpack-merge": "^5.0.0",
    "@types/webpack-node-externals": "^2.5.1",
    "@typescript-eslint/eslint-plugin": "^6.6.0",
    "@typescript-eslint/parser": "^6.6.0",
    "css-loader": "^6.3.0",
    "eslint": "^8.48.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-react-hooks": "^4.2.0",
    "html-webpack-plugin": "^5.3.1",
    "jest": "^29.6.4",
    "nodecg-cli": "^8.0.1",
    "style-loader": "^3.3.0",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.4.4",
    "ts-node": "^10.0.0",
    "ts-nodecg": "^0.3.1",
    "typescript": "^5.2.2",
    "webpack": "^5.38.1",
    "webpack-cli": "^4.7.0",
    "webpack-merge": "^5.8.0",
    "webpack-node-externals": "^3.0.0"
  }
}
